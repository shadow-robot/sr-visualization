<package>
  <description brief="sr_gui_change_controllers">

     sr_gui_change_controllers is a rosgui plugin for loading the different controllers.

  </description>
  <author>Ugo Cupcic</author>
  <license>GPL</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/sr_gui_controller_tuner</url>
  <depend package="rospy"/>
  <depend package="rqt_gui"/>
  <depend package="rqt_gui_py"/>
  <depend package="pr2_mechanism_msgs"/>
  <depend package="sr_robot_msgs"/>

  <export>
    <rqt_gui plugin="${prefix}/sr_change_controllers_plugin.xml"/>
  </export>

</package>


