<package>
  <description brief="sr_gui_self_test">

     This is a rosgui plugin for running self diagnostics of a robot.

  </description>
  <author>Ugo Cupcic</author>
  <license>GPL</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/sr_gui_self_test</url>
  <depend package="rospy"/>
  <depend package="rqt_gui"/>
  <depend package="rqt_gui_py"/>
  <depend package="diagnostic_msgs"/>
  <depend package="sr_robot_msgs"/>

  <export>
    <rqt_gui plugin="${prefix}/sr_self_test_plugin.xml"/>
  </export>

</package>


