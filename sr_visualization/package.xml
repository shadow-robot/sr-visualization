<package>
  <name>sr_visualization</name>
  <version>1.3.1</version>
  <description>This stack contains the different gui plugins used with the shadow robot stacks.</description>
  <maintainer email="software@shadowrobot.com">Shadow Robot's software team</maintainer>

  <license>GPL</license>
  <license>BSD</license>

  <url type="website">http://ros.org/wiki/sr_visualization</url>
  <url type="repository">https://github.com/shadow-robot/sr-visualization</url>
  <url type="bugtracker">https://github.com/shadow-robot/sr-visualization/issues</url>

  <author email="software@shadowrobot.com">Maintained by Ugo Cupcic </author>

  
  <buildtool_depend>catkin</buildtool_depend>

  
  <run_depend>sr_gui_controller_tuner</run_depend>
  <run_depend>sr_gui_change_controllers</run_depend>
  <run_depend>sr_gui_grasp_controller</run_depend>
  <run_depend>sr_visualization_icons</run_depend>

  <export>
    <metapackage />
  </export>
</package>
