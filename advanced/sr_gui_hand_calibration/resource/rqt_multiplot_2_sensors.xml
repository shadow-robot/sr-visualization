                            </y_axis>
                        </axes>
                    </axes>
                    <curves>
                        <curve_0>
                            <axes>
                                <x_axis>
                                    <field></field>
                                    <field_type>1</field_type>
                                    <scale>
                                        <absolute_maximum>1000</absolute_maximum>
                                        <absolute_minimum>0</absolute_minimum>
                                        <relative_maximum>0</relative_maximum>
                                        <relative_minimum>-30</relative_minimum>
                                        <type>2</type>
                                    </scale>
                                    <topic>/rh/debug_etherCAT_data</topic>
